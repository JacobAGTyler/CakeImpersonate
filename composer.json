{
    "name": "jomweb/cake-impersonate",
    "description": "Impersonate plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeImpersonate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeImpersonate\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
